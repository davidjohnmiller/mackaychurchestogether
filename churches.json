---
layout: nil
---
{
    "type": "FeatureCollection",
    "features": [
      {% for Church in site.churches %}
        {
            "type": "Feature",
            "geometry": {
                "type": "Point",
                "coordinates": [{{ Church.Coordinates.Latitude }}, {{ Church.Coordinates.Longitude }}]
            },
            "properties": {
                "Name": "{{ Church.Name }}",
                "Denomination": "{{ Church.Denomination }}",
                "Leader": "{{ Church.Leader }}",
                "Service Times": "{{ Church.Service Times }}",
                "marker-size": "large",
                "marker-symbol": "church",
                "marker-color": "#548cba"
            }
        }{% if forloop.last %}{% else %},{% endif %}
      {% endfor %}
    ]
}
